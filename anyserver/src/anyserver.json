{
  "_comment": "",
  "@name": "GameServer",
  "server_list": [
    {
      "type" : "websocket", 
      "enable" : true, 
      "bind" : 10000, 
      "tcp" : true,
      "max_client" : 200
    },    
    {
      "type" : "http",
      "enable" : true, 
      "bind" : 10001, 
      "tcp" : true,
      "max_client" : 200
    }, 
    { 
      "type" : "inet_domainsocket", 
      "enable" : true, 
      "bind" : 10002, 
      "tcp" : true,
      "max_client" : 200
    },
    {
      "type" : "inet_domainsocket", 
      "enable" : true, 
      "bind" : 10002, 
      "tcp" : false,
      "max_client" : 200
    },
    { 
      "type" : "unix_domainsocket", 
      "enable" : true, 
      "bind" : "/tmp/uds_tcp.tmp", 
      "tcp" : true,
      "max_client" : 200
    },
    { 
      "type" : "unix_domainsocket", 
      "enable" : true, 
      "bind" : "/tmp/uds_udp.tmp", 
      "tcp" : false,
      "max_client" : 200
    }
  ],
  "capabilities" :
  {
    "security" : false, 
    "ssl_cert_files" : [
      { "path" : "test.pem" }
    ],
    "ssl_private_key_files" : [
      { "path" : "test.key.pem" }
    ],
    "ssl_ca_files" : [
      { "path" : "test.ca" }
    ]
  },
  "log" : 
  {
    "enable" : true,
    "file" : "./log.txt"
  },
  "test" :
  {
    "echo" : true
  },
  "version": "0.0.1",
  "copyright": "All right reserved by TehoKang"
}