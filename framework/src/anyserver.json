{
  "_comment": "",
  "@name": "GameServer",
  "type":
  {
    "websocket" : 
    {
      "enable" : false,
      "bind" : 9999,
      "tcp" : true
    },    
    "http" : 
    {
      "enable" : false,
      "bind" : 9999, 
      "tcp" : true
    }, 
    "inet_domainsocket" : 
    {
      "enable" : true,
      "bind" : 9999, 
      "tcp" : true
    },
    "unix_domainsocket" :
    {
      "enable" : false,
      "bind" : "uds.tmp",
      "tcp" : true
    }
  },
  
  "capabilities" :
  {
    "max_client" : 200,
    "security" : false
  },
  "log" : 
  {
    "enable" : true,
    "file" : "./log.txt"
  },
