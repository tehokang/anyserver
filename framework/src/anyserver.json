    { "type" : "unix_domainsocket", "enable" : true, "bind" : "/tmp/uds_tcp.tmp", "tcp" : true },
    { "type" : "unix_domainsocket", "enable" : true, "bind" : "/tmp/uds_udp.tmp", "tcp" : false }
  ],
  "capabilities" :
  {
    "max_client" : 200,
    "security" : false
  },
  "log" : 
  {
    "enable" : true,
    "file" : "./log.txt"
  },
